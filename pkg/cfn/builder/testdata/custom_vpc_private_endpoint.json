{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "VPCEndpointEC2": {
            "Type": "AWS::EC2::VPCEndpoint",
            "Properties": {
                "PrivateDnsEnabled": true,
                "SecurityGroupIds": [
                    "sg-test"
                ],
                "ServiceName": "com.amazonaws.us-west-2.ec2",
                "SubnetIds": [
                    "subnet-custom1",
                    "subnet-custom2"
                ],
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Fn::Sub": "${AWS::StackName}/VPCEndpointEC2"
                        }
                    }
                ],
                "VpcEndpointType": "Interface",
                "VpcId": "vpc-custom"
            }
        },
